acing: 10px;\n}\n.section-title > span{\n    background-color: white ;\n}\n.skills-list{\n    display: flex;\n    flex-direction: column;\n    gap: 16px;\n    flex-wrap: wrap;\n    align-self: flex-start;\n    font-size: 20px;\n    list-style: none;\n}\n.skills-list__name:before {\n    content: \"✔\";\n    color: darkgreen;\n    margin-right: 8px;\n}\n.skills-icon{\n    display: flex;\n    height: 120px;\n    width: 120px;\n    border: gold solid 2px;\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.3);\n    border-radius: 60px;\n\n}\n.skills-container{\n    display: flex;\n    align-self: flex-start;\n    gap: 800px;\n}\n.experience{\n    display: flex;\n    margin-top: 24px;\n    flex-direction: column;\n    border: darkgoldenrod 1px solid;\n    align-items: center;\n    border-radius: 40px;\n}\n.experience-description>span{\n    display: flex;\n    text-align: center;\n    align-items: center;\n    margin: 20px auto;\n    font-size: 20px;\n    line-height: 2;\n    max-width: 70%;\n    background-color: #FFEFD5;\n}\n.languages {\n    display: flex;\n    margin-top: 24px;\n    flex-direction: column;\n    border: darkgoldenrod 1px solid;\n    align-items: center;\n    border-radius: 40px;\n}\n.languages-list {\n    display: flex;\n    flex-direction: row;\n    gap: 200px;\n    list-style: none;\n    font-size: 20px;\n}\n.languages-list__name:before {\n    border: 2px solid ;\n    border-radius: 30px;\n    padding: 1px 5px;\n    content: \"☆\";\n    color: darkgreen;\n    margin-right: 8px;\n}\n.education {\n    display: flex;\n    margin-top: 24px;\n    flex-direction: column;\n    border: darkgoldenrod 1px solid;\n    align-items: center;\n    border-radius: 40px;\n}\n.education-description {\n    display: flex;\n    gap: 100px;\n    list-style: none;\n    font-size: 20px;\n    padding: 20px;\n}\n\n.education-description > div:nth-child(odd){\n    font-weight: bold;\n}\n.footer {\n    display: flex;\n    background-color:#FFEFD5 ;\n    justify-content: space-between;\n 